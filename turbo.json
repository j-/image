{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "dev": {},
    "start": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "inputs": [
        "*.ts",
        "*.tsx",
        "**/*.ts",
        "**/*.tsx"
      ]
    }
  }
}
